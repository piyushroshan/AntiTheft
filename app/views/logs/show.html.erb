<div class="container">
<%- model_class = Log -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>
<p id="notice"><%= notice %></p>
<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ip_address) %>:</strong></dt>
  <dd><%= @log.ip_address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:device_id) %>:</strong></dt>
  <dd><%= @log.device_id %></dd>
  <dt><strong>Log File:</strong></dt>
  <dd><%= @log.log_file_url() %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              logs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_log_path(@log), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              log_path(@log),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
</div>